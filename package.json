{
	"name": "svelte-commerce",
	"version": "2023.05.06",
	"private": true,
	"scripts": {
		"dev": "node src/lib/services/copy && vite dev --host --port 3000",
		"build": "node src/lib/services/copy && vite build",
		"start-node": "node build/index.js",
		"start": "NODE_ENV=production && vite preview --host --port 3000",
		"update": "npm exec -- npx ncu -u -p npm i",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"partytown": "partytown copylib static/~partytown",
		"gen:manifest": "node ./generate-manifest.js",
		"copy": "node devops/copy",
		"liveS": "node devops/deploy",
		"live": "run-s build copy liveS"
	},
	"dependencies": {
		"@vite-pwa/sveltekit": "0.2.1"
	},
	"devDependencies": {
		"@beyonk/svelte-facebook-pixel": "3.0.1",
		"@beyonk/svelte-google-analytics": "2.6.4",
		"@builder.io/partytown": "0.8.0",
		"@playwright/test": "^1.33.0",
		"@sentry/node": "7.51.0",
		"@sentry/svelte": "7.51.0",
		"@sentry/tracing": "7.51.0",
		"@splidejs/splide-extension-video": "0.8.0",
		"@splidejs/svelte-splide": "0.2.9",
		"@sveltejs/adapter-auto": "2.0.1",
		"@sveltejs/adapter-vercel": "2.4.3",
		"@sveltejs/kit": "1.16.2",
		"@tailwindcss/line-clamp": "^0.4.4",
		"@tailwindcss/typography": "^0.5.9",
		"@types/bcrypt": "^5.0.0",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"@woocommerce/woocommerce-rest-api": "1.0.1",
		"amazon-s3-uri": "^0.1.1",
		"autoprefixer": "^10.4.14",
		"aws-sdk": "^2.1373.0",
		"bcrypt": "^5.1.0",
		"cookie": "^0.5.0",
		"cookie-universal": "^2.2.2",
		"cssnano": "^6.0.1",
		"date-fns": "2.30.0",
		"dayjs": "^1.11.7",
		"dotenv": "16.0.3",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"fuse.js": "6.6.2",
		"hash-it": "^6.0.0",
		"ioredis": "5.3.2",
		"minio": "7.1.0",
		"npm-run-all": "4.1.5",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"shelljs": "0.8.5",
		"ssh2shell": "2.0.8",
		"svelte": "^3.59.0",
		"svelte-confetti": "^1.2.2",
		"svelte-preprocess": "^5.0.3",
		"svelte-share-buttons-component": "1.5.5",
		"svelte-timeago": "0.1.2",
		"svelte-toasts": "^1.1.2",
		"sveltekit-medusa-client": "0.0.1",
		"tailwind-scrollbar": "3.0.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.0",
		"typescript": "~5.0.4",
		"vanilla-lazyload": "^17.8.3",
		"vite": "^4.3.5",
		"vite-plugin-pwa": "0.14.7",
		"zod": "3.21.4",
		"zod-form-data": "2.0.1"
	},
	"type": "module"
}